class Fib {
  public static long fibonacci(int n)
  {
    if(n==0)
    {
      return 0;
    }
    else if(n==1)
    {
      return 1;
    }
    else
    {
      int n1=0;
      int n2=1;
      int sum;
      for(int i=2;i<=n;i++)
      {
    	  sum = n1 + n2;
          n1 = n2;
          n2 = sum;
        
      }
      System.out.println(n2);
      return n2;
    }
  }
